<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="game.controller.MenuController">
   <children>
      <Button fx:id="startButton" layoutX="169.0" layoutY="181.0" mnemonicParsing="false" onAction="#startGame" text="Start">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Text fx:id="gameTitel" layoutX="34.0" layoutY="111.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Minespeeper">
         <font>
            <Font name="Comic Sans MS" size="56.0" />
         </font>
      </Text>
      <TextField fx:id="sizeField" layoutX="170.0" layoutY="227.0" prefHeight="25.0" prefWidth="61.0" promptText="Type in size..." text="10" />
      <Label fx:id="textFieldCorrectnessLabel" layoutX="171.0" layoutY="252.0" prefHeight="17.0" prefWidth="210.0" textFill="RED">
         <font>
            <Font size="10.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
